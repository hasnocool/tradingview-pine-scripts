computeVWAP(src20,Â isNewPeriod,Â stDevMultiplier)Â =>
Â Â Â Â varÂ floatÂ sumSrcVolÂ =Â na
Â Â Â Â varÂ floatÂ sumVolÂ =Â na
Â Â Â Â varÂ floatÂ sumSrcSrcVolÂ =Â na
Â Â Â Â 
Â Â Â Â sumSrcVolÂ :=Â isNewPeriodÂ ?Â src20Â *Â volumeÂ :Â src20Â *Â volumeÂ +Â sumSrcVol[1]
Â Â Â Â sumVolÂ :=Â isNewPeriodÂ ?Â volumeÂ :Â volumeÂ +Â sumVol[1]
Â Â Â Â //Â sumSrcSrcVolÂ calculatesÂ theÂ dividendÂ ofÂ theÂ equationÂ thatÂ isÂ laterÂ usedÂ toÂ calculateÂ theÂ standardÂ deviation
Â Â Â Â sumSrcSrcVolÂ :=Â isNewPeriodÂ ?Â volumeÂ *Â pow(src20,Â 2)Â :Â volumeÂ *Â pow(src20,Â 2)Â +Â sumSrcSrcVol[1]
Â Â Â Â 
Â Â Â Â _vwapÂ =Â sumSrcVolÂ /Â sumVol
Â Â Â Â varianceÂ =Â sumSrcSrcVolÂ /Â sumVolÂ -Â pow(_vwap,Â 2)
Â Â Â Â varianceÂ :=Â varianceÂ <Â 0Â ?Â 0Â :Â variance
Â Â Â Â stDevÂ =Â sqrt(variance)
Â Â Â Â 
Â Â Â Â lowerBand20Â =Â _vwapÂ -Â stDevÂ *Â stDevMultiplier
Â Â Â Â upperBand20Â =Â _vwapÂ +Â stDevÂ *Â stDevMultiplier
